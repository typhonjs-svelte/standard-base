{
  "name": "@typhonjs-svelte/standard-base",
  "version": "0.3.0",
  "description": "Provides a standard base Svelte component library for the TyphonJS Runtime Library.",
  "type": "module",
  "author": "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)",
  "contributors": [
    "Michael Leahy <support@typhonjs.io> (https://github.com/typhonrt)"
  ],
  "imports": {
    "#runtime/*": "@typhonjs-svelte/runtime-base/*",
    "#standard/*": {
      "types": "./_dist/*/index.d.ts",
      "default": "@typhonjs-svelte/standard-base/*"
    },
    "#svelte/*": "svelte/*",
    "#svelte": "svelte"
  },
  "exports": {
    "./action/animate/composable": {
      "types": "./_dist/action/animate/composable/index.d.ts",
      "import": "./_dist/action/animate/composable/index.js"
    },
    "./application/menu": {
      "types": "./_dist/application/menu/index.d.ts",
      "import": "./_dist/application/menu/index.js"
    },
    "./component/button": {
      "types": "./_dist/component/button/index.d.ts",
      "import": "./_dist/component/button/index.js"
    },
    "./component/color/picker": {
      "types": "./_dist/component/color/picker-colord/index.d.ts",
      "import": "./_dist/component/color/picker-colord/index.js"
    },
    "./component/container": {
      "types": "./_dist/component/container/index.d.ts",
      "import": "./_dist/component/container/index.js"
    },
    "./component/dom/focus": {
      "types": "./_dist/component/dom/focus/index.d.ts",
      "import": "./_dist/component/dom/focus/index.js"
    },
    "./component/folder": {
      "types": "./_dist/component/folder/index.d.ts",
      "import": "./_dist/component/folder/index.js"
    },
    "./component/form": {
      "types": "./_dist/component/form/index.d.ts",
      "import": "./_dist/component/form/index.js"
    },
    "./component/label": {
      "types": "./_dist/component/label/index.d.ts",
      "import": "./_dist/component/label/index.js"
    },
    "./component/layer/position": {
      "types": "./_dist/component/layer/position/index.d.ts",
      "import": "./_dist/component/layer/position/index.js"
    },
    "./component/layer/side-slide": {
      "types": "./_dist/component/layer/side-slide/index.d.ts",
      "import": "./_dist/component/layer/side-slide/index.js"
    },
    "./component/media": {
      "types": "./_dist/component/media/index.d.ts",
      "import": "./_dist/component/media/index.js"
    },
    "./component/menu": {
      "types": "./_dist/component/menu/index.d.ts",
      "import": "./_dist/component/menu/index.js"
    },
    "./package.json": "./package.json"
  },
  "peerDependencies": {
    "svelte": ">=4.x.x <5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@typhonjs-build-test/esm-d-ts": "^0.3.0-next.11",
    "@typhonjs-build-test/esm-d-ts-plugin-svelte": "^0.1.0-next.1",
    "@typhonjs-build-test/rollup-plugin-pkg-imports": "^0.6.0",
    "@typhonjs-config/eslint-config": "^0.6.3",
    "@typhonjs-svelte/runtime-base": "github:typhonjs-svelte/runtime-base#0.3.0",
    "@typhonjs-utils/file-util": "^0.1.0",
    "eslint": "^8.57.0",
    "fs-extra": "^11.3.0",
    "rollup": "^4.40.2"
  },
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/typhonjs-svelte/standard-base#readme",
  "keywords": [
    "typhonjs",
    "trl",
    "svelte",
    "components"
  ],
  "scripts": {
    "build": "node rollupAll.js",
    "eslint": "eslint ."
  },
  "files": [
    "/_dist"
  ],
  "directories": {},
  "license": "MPL-2.0",
  "repository": "github:typhonjs-svelte/standard-base",
  "bugs": "https://github.com/typhonjs-svelte/standard-base/issues"
}
